{
  "version": "2.1.0",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "dom", "es2015.promise", "es5"
    ],
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": false,
    "noResolve": true,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "experimentalDecorators": true
  },
  "exclude": [
    "build",
    "node_modules",
    "static",
    "gulpfile.ts"
  ]
}
