- var notice = "<a href='/#/faq/notice'>notices</a>";

template
  section.changelog.optiscroll-content(class='${router.isNavigating ? "hidden" : "visible"}')
    section#ChangeList
      span.title(repeat.for='log of logs'
                 class='${clicked ? "hidden" : "visible"}'
                 click.delegate='open(log.timeStamp)')
        div.date ${log.date}
        div.description ${log.title}

    article#Changes(innerhtml.bind='changes', class='${clicked ? "visible" : "hidden"}')

    aside.close.icon-close(click.delegate='close()', class='${clicked ? "visible" : "hidden"}')




  div.cssload-container(if.bind='router.isNavigating')
    div.cssload-circle
    div.cssload-circle
