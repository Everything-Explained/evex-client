{
    "rules": {
        "class-name": true,
        "comment-format": [true,
            "check-space"
        ],
        "indent": [true,
            "spaces"
        ],
        "no-unreachable": true,
        "no-use-before-declare": true,
        "no-var-keyword": true,
        "no-var-requires": false,
        "quotemark": [true,
            "single",
            "avoid-escape"
        ],
        "trailing-comma": [true,
          "mutliline",
          "singleline"
        ],
        "semicolon": [true, "always"],
        "whitespace": [true,
            "check-branch",
            "check-operator",
            "check-separator",
            "check-type"
        ],
        "typedef-whitespace": [true, {
            "call-signature": "nospace",
            "index-signature": "nospace",
            "parameter": "nospace",
            "property-declaration": "nospace",
            "variable-declaration": "nospace"
        }],
        "interface-name": [true, "always-prefix"]
  }
}
