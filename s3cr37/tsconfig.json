{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "noResolve": true,
    "lib": [
      "dom",
      "es5",
      "es2015.promise"
    ],
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "experimentalDecorators": true,
    "watch": true
  }
}